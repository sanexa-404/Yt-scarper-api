{
  "name": "music-api",
  "version": "1.0.0",
  "description": "REST API for streaming music from YouTube - optimized for Flutter apps",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest --testPathPattern=tests/ --passWithNoTests",
    "test:watch": "jest --watch",
    "lint": "eslint src/",
    "format": "prettier --write \"src/**/*.js\"",
    "deploy": "npm run build && npm start",
    "build": "echo 'No build step required'",
    "render:deploy": "npm start"
  },
  "keywords": [
    "music",
    "api",
    "youtube",
    "streaming",
    "rest-api",
    "flutter",
    "audio",
    "music-player",
    "nodejs",
    "express"
  ],
  "author": "Your Name <sanexa025@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanexa-404/music-api.git"
  },
  "homepage": "https://github.com/sanexa-404/music-api#readme",
  "bugs": {
    "url": "https://github.com/sanexa-404/music-api/issues"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "rate-limiter-flexible": "^3.0.4",
    "winston": "^3.11.0",
